<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://sbolstandard.org/visual"
         xmlns="http://sbolstandard.org/visual#">

<owl:Ontology rdf:about="http://sbolstandard.org/visual">
  <owl:imports rdf:resource="https://dissys.github.io/sbol-owl/sbol.rdf"/>
</owl:Ontology>

<owl:Class rdf:about="#Glyph">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:ObjectProperty rdf:about="#isGlyphOf"/>

<owl:Class rdf:about="#InsulatorGlyph">
  <rdfs:subClassOf rdf:resource="#Glyph"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Glyph"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#isGlyphOf"/>
          <owl:someValuesFrom>
            <owl:Class>
              <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="http://sbolstandard.org/v2#ComponentDefinition"/>
                <owl:Restriction>
                  <owl:onProperty rdf:resource="http://sbolstandard.org/v2#role"/>
                  <owl:someValuesFrom rdf:resource="http://identifiers.org/so/SO_0000167"/>
                </owl:Restriction>
              </owl:intersectionOf>
            </owl:Class>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#InsulatorGlyph2">
  <rdfs:subClassOf rdf:resource="#Glyph"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Glyph"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#isGlyphOf"/>
          <owl:someValuesFrom>
            <owl:Class>
              <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="http://sbolstandard.org/v2#ComponentDefinition"/>
                <owl:Restriction>
                  <owl:onProperty rdf:resource="http://sbolstandard.org/v2#role"/>
                  <owl:someValuesFrom rdf:resource="http://identifiers.org/so/SO_0000167"/>
                </owl:Restriction>
              </owl:intersectionOf>
            </owl:Class>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#AptamerGlyph">
  <rdfs:subClassOf rdf:resource="#Glyph"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Glyph"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#isGlyphOf"/>
          <owl:someValuesFrom>
            <owl:Class>
              <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="http://sbolstandard.org/v2#ComponentDefinition"/>
                <owl:Restriction>
                  <owl:onProperty rdf:resource="http://sbolstandard.org/v2#role"/>
                  <owl:someValuesFrom>
                    <owl:Class>
                      <owl:unionOf rdf:parseType="Collection">
                        <rdf:Description rdf:about="http://identifiers.org/so/SO_0000031"/>
                        <rdf:Description rdf:about="http://identifiers.org/so/SO_0000031_gm2"/>
                        <rdf:Description rdf:about="http://identifiers.org/so/SO_0000031_gm3"/>
                      </owl:unionOf>
                    </owl:Class>
                  </owl:someValuesFrom>
                </owl:Restriction>
              </owl:intersectionOf>
            </owl:Class>
          </owl:someValuesFrom>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>


</rdf:RDF>
