<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://sbolstandard.org/visual"
         xmlns="http://sbolstandard.org/visual#">

<owl:Ontology rdf:about="http://sbolstandard.org/visual"/>

<owl:Class rdf:about="#Drug">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Ingredient">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:ObjectProperty rdf:about="#has_for_ingredient">
  <rdfs:domain rdf:resource="#Drug"/>
  <rdfs:range rdf:resource="#Ingredient"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#TestDrug">
  <rdfs:subClassOf rdf:resource="#Drug"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Drug"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#has_for_ingredient"/>
          <owl:someValuesFrom rdf:resource="#Ingredient"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Glyph">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:ObjectProperty rdf:about="#isGlyphOf"/>

<owl:Class rdf:about="#InsulatorGlyph">
  <rdfs:subClassOf rdf:resource="#Glyph"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Glyph"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#isGlyphOf"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>


</rdf:RDF>
